#include <stdio.h>
#include "system.h"
#include "altera_avalon_pio_regs.h"
#include "alt_types.h"

#define sdram_rw_addr (alt_u8)SDRAM0_BASE + 0x1000000)

int main()
{
	alt_u8 *addr;
	alt_u8 led=0, key=0;
	printf("Hello world!\n");
	while(1)
	{
		key = IORD_ALTERA_AVALON_PIO_DATA(PIO_KEY_BASE);
		led = ~key;
		IOWR_ALTERA_AVALON_PIO_DATA(PIO_LED_BASE, led);
	}
	return 0;
}
